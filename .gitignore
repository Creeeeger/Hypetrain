# Ignore the 'target' directory commonly used by Maven for build outputs,
# but allow the Maven wrapper JAR inside '.mvn/wrapper/'
target/

# Allow Maven wrapper JAR file (required for builds)
!.mvn/wrapper/maven-wrapper.jar

# Allow 'target' directories inside source code and test folders
!**/src/main/**/target/
!**/src/test/**/target/

### IntelliJ IDEA ###
# Ignore IDE configuration files and folders generated by IntelliJ IDEA
.idea/modules.xml
.idea/jarRepositories.xml
.idea/compiler.xml
.idea/libraries/
*.iws      # IntelliJ workspace files
*.iml      # IntelliJ module files
*.ipr      # IntelliJ project files

### Eclipse ###
# Ignore Eclipse-specific project files and generated folders
.apt_generated
.classpath
.factorypath
.project
.settings
.springBeans
.sts4-cache

### NetBeans ###
# Ignore NetBeans project private configuration and build folders
/nbproject/private/
/nbbuild/
/dist/
/nbdist/
/.nb-gradle/

# Ignore general build directory, but allow build directories inside source and test folders
build/
!**/src/main/**/build/
!**/src/test/**/build/

### VS Code ###
# Ignore VS Code workspace settings and extensions folder
.vscode/

### macOS ###
# Ignore macOS system files and logs
.DS_Store
*log

# Ignore configuration XML files
config.xml

# ignore cache
cache/
!ToDO.txt
.txt
/cache/